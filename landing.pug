block script
    script(src="js/simplyCountdown.min.js")
    script(src="https://cdn.jsdelivr.net/npm/typed.js@2.0.11")
    script(src="js/jquery-3.5.1.min.js" type="text/javascript")
    script(src="js/app.js" type="text/javascript")
block contenidos
        main#main
            include pug_components/dynamic.pug
            include pug_components/front.pug
            block frontal
            .container-fluid.camp
                .row.row-cols-1.row-cols-md-3.g-4
                    +CampamentoActivo(0, 1)
                    +CampamentoInactivo('java')
                    +CampamentoInactivo('c')
            include pug_components/about.pug
            include pug_components/tribute.pug